{% extends 'admin/index.html.twig' %}

{% block isActiveLocation %}active{% endblock %}

{% block body %}
    <div class="row justify-content-center">

        <div class="col-md-6">
            <h3>Recherche</h3>
            {{ form_start(form) }}
            {{ form_rest(form) }}
            <button class="btn btn-info">{{ button_label|default('Valider') }}</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}



{% block javascripts %}
    {{ parent() }}
    <script>

        $('#search_equipment').autocompleter({
            url_list: '/member/equipment_search',
            url_get: '/member/equipment_get'
        });
    </script>
{% endblock %}
